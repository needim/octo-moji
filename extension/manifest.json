{
  "manifest_version": 2,
  "name": "Octo-Complete",
  "version": "1.0",
  "description": "Github emoji auto-complete",
  "homepage_url": "https://github.com/sym3tri/octo-complete",

  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },

  "content_scripts": [{
    "matches": ["https://github.com/*"],
    "css": ["style.css"],
    "js": ["octocomplete.js"]
  }],

  "web_accessible_resources": ["emoji.json"]
}
